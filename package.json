{
  "name": "mightyminer-nodejs",
  "version": "3.0.0",
  "description": "MightyMiner - Complete Node.js/Mineflayer automation system with Discord integration and VPS deployment",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon src/MightyMiner.js",
    "test": "node test/test.js",
    "discord": "node discord-bot/index.js",
    "build": "docker build -t mightyminer .",
    "deploy": "./vps/deploy.sh",
    "update": "./vps/update.sh",
    "health": "node healthcheck.js",
    "backup": "./backup.sh",
    "logs": "pm2 logs mightyminer",
    "status": "pm2 status",
    "restart": "pm2 restart mightyminer",
    "stop": "pm2 stop mightyminer"
  },
  "dependencies": {
    "mineflayer": "^4.19.1",
    "mineflayer-pathfinder": "^2.4.1",
    "mineflayer-collectblock": "^1.6.1",
