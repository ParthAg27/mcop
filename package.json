    "mineflayer-armor-manager": "^1.3.1",
    "mineflayer-web-inventory": "^1.8.0",
    "vec3": "^0.1.8",
    "prismarine-block": "^1.17.1",
    "prismarine-item": "^1.13.1",
    "minecraft-data": "^3.58.0",
    "lodash": "^4.17.21",
    "fs-extra": "^11.1.1",
    "discord.js": "^14.14.1",
    "axios": "^1.6.0",
    "uuid": "^9.0.1",
    "dotenv": "^16.3.1",
    "winston": "^3.11.0",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "compression": "^1.7.4",
    "redis": "^4.6.0",
    "sharp": "^0.32.6",
    "canvas": "^2.11.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
